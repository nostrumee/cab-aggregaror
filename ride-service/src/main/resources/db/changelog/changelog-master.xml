<databaseChangeLog xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                   http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.3.xsd">

    <preConditions>
        <dbms type="postgresql"/>
    </preConditions>

    <changeSet id="0" author="Eduard Klygunov">
        <tagDatabase tag="db_init"/>
    </changeSet>

    <include file="v1/v1-accumulate-changelog.xml" relativeToChangelogFile="true"/>

    <changeSet id="2" author="Eduard Klygunov">
        <tagDatabase tag="v.1.0"/>
    </changeSet>

    <include file="v2/v2-accumulate-changelog.xml" relativeToChangelogFile="true"/>

    <changeSet id="4" author="Eduard Klygunov">
        <tagDatabase tag="v.2.0"/>
    </changeSet>

    <include file="v3/v3-accumulate-changelog.xml" relativeToChangelogFile="true"/>

    <changeSet id="6" author="Eduard Klygunov">
        <tagDatabase tag="v.3.0"/>
    </changeSet>

</databaseChangeLog>